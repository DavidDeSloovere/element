{
	"name": "@flood/element-cli",
	"version": "1.0.3-30",
	"license": "Apache-2.0",
	"description": "The command line interface for Flood Element",
	"repository": "flood-io/element",
	"bin": {
		"element": "./dist/index.js"
	},
	"private": false,
	"scripts": {
		"build": "tsc --outDir dist --project .",
		"build:watch": "tsc --watch --outDir dist --project .",
		"prepublishOnly": "npm run build",
		"element": "ts-node -- ./index.ts",
		"element:inspect": "node --inspect-brk -r ts-node/register ./index.ts"
	},
	"types": "./index.d.ts",
	"dependencies": {
		"@flood/element": "^1.0.3-30",
		"chalk": "^2.4.1",
		"command-exists": "^1.2.7",
		"debug": "^3.1.0",
		"find-root": "^1.1.0",
		"ms": "^2.1.1",
		"ora": "^2.1.0",
		"update-check": "^1.5.2",
		"winston": "^3.0.0",
		"yargs": "^11.0.0",
		"yeoman-environment": "^2.3.2",
		"yeoman-generator": "^3.1.1"
	},
	"devDependencies": {
		"@types/chalk": "^2.2.0",
		"@types/ora": "^1.3.4",
		"@types/yargs": "^11.0.0",
		"@types/yeoman-generator": "^2.0.3"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "avoid"
	}
}
