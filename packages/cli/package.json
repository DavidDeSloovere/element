{
	"name": "@flood/cli",
	"version": "1.0.3-3",
	"license": "Apache-2.0",
	"description": "The command line interface for Flood Element",
	"repository": "flood-io/element",
	"bin": {
		"element": "./dist/index.js"
	},
	"private": false,
	"scripts": {
		"build": "tsc --outDir dist --project .",
		"prepublishOnly": "npm run build",
		"element": "ts-node -- ./index.ts",
		"element:inspect": "node --inspect-brk -r ts-node/register ./index.ts"
	},
	"types": "./index.d.ts",
	"dependencies": {
		"@flood/element": "^1.0.3-3",
		"chalk": "^2.4.1",
		"debug": "^3.1.0",
		"ms": "^2.1.1",
		"ora": "^2.1.0",
		"update-check": "^1.5.2",
		"winston": "^3.0.0",
		"yargs": "^11.0.0"
	},
	"devDependencies": {
		"@types/chalk": "^2.2.0",
		"@types/node": "^10.3.4",
		"@types/ora": "^1.3.4",
		"@types/yargs": "^11.0.0",
		"nodemon": "^1.17.5",
		"ts-node": "^6.1.1",
		"typescript": "^2.9.2"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "avoid"
	}
}
