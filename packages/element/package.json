{
	"name": "@flood/element",
	"version": "1.0.3-38",
	"description": "Flood Element is a browser based load generation tool for easily testing modern web application performance",
	"types": "./lib/index.ts.d",
	"scripts": {
		"build": "tsc --project . --declaration",
		"build:watch": "tsc --watch --project .",
		"prepublishOnly": "npm run build",
		"test": "mocha --trace-warnings -r ts-node/register src/**/*.spec.ts",
		"doc": "yarn typedoc && ts-node ./scripts/docs/docs.ts",
		"doc:watch": "node --trace-warnings $(npm bin)/nodemon --ext ts --exec ts-node -- ./scripts/docs/docs.ts",
		"doc:once": "ts-node ./scripts/docs/docs.ts",
		"typedoc": "typedoc --json docs.json ./index.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/flood-io/element.git"
	},
	"keywords": [
		"load",
		"testing",
		"puppeteer",
		"chrome",
		"browser",
		"testing",
		"testing"
	],
	"author": "Ivan Vanderbyl <ivan@flood.io>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/flood-io/element/issues"
	},
	"homepage": "https://github.com/flood-io/element#readme",
	"dependencies": {
		"@flood/element-core": "^1.0.3-38"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "avoid"
	}
}
